<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oit.oitcloud.dao.OitUserOrganizeRelDao">

    <resultMap type="com.oit.oitcloud.entity.OitUserOrganizeRel" id="OitUserOrganizeRelMap">
        <result property="id" column="ID" jdbcType="INTEGER"/>
        <result property="userId" column="USER_ID" jdbcType="INTEGER"/>
        <result property="organizeId" column="ORGANIZE_ID" jdbcType="INTEGER"/>
        <result property="createBy" column="CREATE_BY" jdbcType="VARCHAR"/>
        <result property="createTime" column="CREATE_TIME" jdbcType="TIMESTAMP"/>
        <result property="modifyBy" column="MODIFY_BY" jdbcType="VARCHAR"/>
        <result property="modifyTime" column="MODIFY_TIME" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="OitUserOrganizeRelMap">
        select
          ID, USER_ID, ORGANIZE_ID, CREATE_BY, CREATE_TIME, MODIFY_BY, MODIFY_TIME
        from oit_user_center.oit_user_organize_rel
        where ID = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="OitUserOrganizeRelMap">
        select
          ID, USER_ID, ORGANIZE_ID, CREATE_BY, CREATE_TIME, MODIFY_BY, MODIFY_TIME
        from oit_user_center.oit_user_organize_rel
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="OitUserOrganizeRelMap">
        select
          ID, USER_ID, ORGANIZE_ID, CREATE_BY, CREATE_TIME, MODIFY_BY, MODIFY_TIME
        from oit_user_center.oit_user_organize_rel
        <where>
            <if test="id != null">
                and ID = #{id}
            </if>
            <if test="userId != null">
                and USER_ID = #{userId}
            </if>
            <if test="organizeId != null">
                and ORGANIZE_ID = #{organizeId}
            </if>
            <if test="createBy != null and createBy != ''">
                and CREATE_BY = #{createBy}
            </if>
            <if test="createTime != null">
                and CREATE_TIME = #{createTime}
            </if>
            <if test="modifyBy != null and modifyBy != ''">
                and MODIFY_BY = #{modifyBy}
            </if>
            <if test="modifyTime != null">
                and MODIFY_TIME = #{modifyTime}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into oit_user_center.oit_user_organize_rel(USER_ID, ORGANIZE_ID, CREATE_BY, CREATE_TIME, MODIFY_BY, MODIFY_TIME)
        values (#{userId}, #{organizeId}, #{createBy}, #{createTime}, #{modifyBy}, #{modifyTime})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update oit_user_center.oit_user_organize_rel
        <set>
            <if test="userId != null">
                USER_ID = #{userId},
            </if>
            <if test="organizeId != null">
                ORGANIZE_ID = #{organizeId},
            </if>
            <if test="createBy != null and createBy != ''">
                CREATE_BY = #{createBy},
            </if>
            <if test="createTime != null">
                CREATE_TIME = #{createTime},
            </if>
            <if test="modifyBy != null and modifyBy != ''">
                MODIFY_BY = #{modifyBy},
            </if>
            <if test="modifyTime != null">
                MODIFY_TIME = #{modifyTime},
            </if>
        </set>
        where ID = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from oit_user_center.oit_user_organize_rel where ID = #{id}
    </delete>

    <!--通过OrganizeId删除-->
    <delete id="deleteByOrganizeId">
        delete from oit_user_center.oit_user_organize_rel where ORGANIZE_ID = #{organizeId}
    </delete>

    <!--通过UserId删除-->
    <delete id="deleteByUserId">
        delete from oit_user_center.oit_user_organize_rel where USER_ID = #{userId}
    </delete>

</mapper>